# Enable the cloud profile by default (use "local" to switch to embedded Qdrant).
COMPOSE_PROFILES=cloud

# Required credentials
OPENAI_API_KEY=your-openai-api-key
CHAT_MODEL=gpt-4.1-nano
EMBEDDING_MODEL=text-embedding-ada-002

# Qdrant Cloud configuration (used in cloud profile)
QDRANT_URL=https://your-qdrant-url.cloud.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key

# Observability (optional)
LANGCHAIN_TRACING_V2=1
LANGSMITH_PROJECT=rag-research-agent
LANGCHAIN_PROJECT=rag-research-agent
LANGCHAIN_API_KEY=
LANGCHAIN_ENDPOINT=

# Alternative model providers (optional)
ANTHROPIC_API_KEY=
FIREWORKS_API_KEY=

# Docker build knobs
PYTHON_VERSION=3.11
PRELOAD_MODELS=0
